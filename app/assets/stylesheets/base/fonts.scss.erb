// Fontspring @Font-Face Syntax
// http://www.fontspring.com/blog/the-new-bulletproof-font-face-syntax

@font-face {
	font-family: 'Signify';
	src: url('<%= asset_path('signifylite.eot')+"?#iefix" %>') format('embedded-opentype'),
	     url('<%= asset_path('signifylite.woff') %>') format('woff'), 
	     url('<%= asset_path('signifylite.ttf') %>')  format('truetype'),
	     url('<%= asset_path('signifylite.svg')+"#Signify" %>') format('svg');
}
